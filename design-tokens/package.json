{
	"name": "design-tokens",
	"description": "utils to consume JSON design token data and generate SCSS vars, CSS vars, and TS types",
	"version": "1.0.0",
	"license": "MIT",
	"ignore-main": "index.html",
	"main": "utils/generator.write.js",
	"scripts": {
		"start": "parcel index.html --open",
		"build": "parcel build index.html",
		"generate": "node utils/generator.write.js"
	},
	"dependencies": {
		"cross-env": "5.0.5",
		"parcel-bundler": "^1.6.1"
	},
	"devDependencies": {
		"@types/svgo": "^2.6.2",
		"extract-svg-path": "^2.1.0",
		"fs-extra": "^5.0.0",
		"path-that-svg": "^1.2.4",
		"svgo": "^1.3.2",
		"typescript": "^4.5.5"
	},
	"resolutions": {
		"@babel/preset-env": "7.13.8"
	},
	"prettier": {
		"trailingComma": "es5",
		"singleQuote": true,
		"tabWidth": 4,
		"useTabs": true
	}
}
