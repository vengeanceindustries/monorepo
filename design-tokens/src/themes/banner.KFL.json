{
	"site": "KFL",
	"name": "Kids Foot Locker",
	"font": {
		"family": {
			"base": "Roboto",
			"heading": "Poppins",
			"mono": "RobotoMono"
		}
	},
	"color": {
		"primary": "{color.science_blue}",
		"secondary": "{color.mineshaft}",
		"highlight": "{color.science_blue}"
	},
	"theme": {
		"light": {
			"css": {
				"color": "{color.black}",
				"background-color": "{color.white}"
			},
			"button": {
				"primary": {
					"color": "{color.black}",
					"border-color": "{color.white}",
					"background-color": "{color.white}"
				},
				"secondary": {
					"color": "{color.white}",
					"border-color": "{color.white}",
					"background-color": "{color.black}"
				}
			}
		},
		"dark": {
			"color": "{color.white}",
			"background-color": "{color.mineshaft}"
		},
		"accent": {
			"css": {
				"color": "{color.white}",
				"background-color": "var(--color-primary)"
			},
			"button": {
				"primary": {
					"color": "var(--color-primary)",
					"border-color": "{color.white}",
					"background-color": "{color.white}"
				},
				"secondary": {
					"color": "{color.white}",
					"border-color": "{color.white}",
					"background-color": "var(--color-primary)"
				}
			}
		}
	}
}
