{
	"site": "'KFL'",
	"name": "'Kids Foot Locker'",
	"font-family": {
		"base": "Roboto",
		"heading": "Poppins",
		"mono": "RobotoMono"
	},
	"color": {
		"primary": "$science_blue",
		"secondary": "$mineshaft",
		"highlight": "$science_blue"
	},
	"theme": {
		"light": {
			"css": {
				"color": "$black",
				"background-color": "$white"
			},
			"button": {
				"primary": {
					"color": "$black",
					"border-color": "$white",
					"background-color": "$white"
				},
				"secondary": {
					"color": "$white",
					"border-color": "$white",
					"background-color": "$black"
				}
			}
		},
		"dark": {
			"color": "$white",
			"background-color": "$mineshaft"
		},
		"accent": {
			"color": "$white",
			"background-color": "var(--color-primary)"
		}
	}
}
