{
	"outline": {
		"light": "$mercury",
		"neutral": "$gray",
		"dark": "$mineshaft"
	},
	"theme": {
		"light": {
			"container": {
				"color": "var(--theme-light-container-color)",
				"background-color": "var(--theme-light-container-background-color)"
			},
			"button": {
				"primary": {
					"color": "var(--theme-light-button-primary-color)",
					"border-color": "var(--theme-light-button-primary-border-color)",
					"background-color": "var(--theme-light-button-primary-background-color)"
				},
				"secondary": {
					"color": "var(--theme-light-button-secondary-color)",
					"border-color": "var(--theme-light-button-secondary-border-color)",
					"background-color": "var(--theme-light-button-secondary-background-color)"
				}
			}
		},
		"dark": {
			"container": {
				"color": "var(--theme-dark-container-color)",
				"background-color": "var(--theme-dark-container-background-color)"
			},
			"button": {
				"primary": {
					"color": "var(--theme-dark-button-primary-color)",
					"border-color": "var(--theme-dark-button-primary-border-color)",
					"background-color": "var(--theme-dark-button-primary-background-color)"
				},
				"secondary": {
					"color": "var(--theme-dark-button-secondary-color)",
					"border-color": "var(--theme-dark-button-secondary-border-color)",
					"background-color": "var(--theme-dark-button-secondary-background-color)"
				}
			}
		}
	}
}
